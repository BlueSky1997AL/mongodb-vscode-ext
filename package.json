{
    "name": "auto-mongodb",
    "displayName": "Auto MongoDB",
    "icon": "asset/logo.png",
    "description": "One click to run MongoDB without installation or configuration",
    "version": "0.0.1",
    "publisher": "AllenLawrence",
    "repository": "https://github.com/BlueSky1997AL/mongodb-vscode-ext",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showMenu",
                "title": "Show Main Menu"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "babel-runtime": "^6.26.0",
        "clipboardy": "^1.2.3",
        "debug": "^3.1.0",
        "decompress": "^4.2.0",
        "fs-extra": "^6.0.1",
        "get-port": "^3.2.0",
        "getos": "^3.1.0",
        "lockfile": "^1.0.4",
        "md5-file": "^4.0.0",
        "mkdirp": "^0.5.1",
        "opn": "^5.3.0",
        "raven": "^2.6.2",
        "request": "^2.87.0",
        "request-promise": "^4.2.2",
        "tmp": "0.0.33",
        "uuid": "^3.2.1"
    }
}
