{
    "name": "auto-mongodb",
    "displayName": "auto-mongodb",
    "description": "One click to manage MongoDB's running status (without installation or configuration)",
    "version": "0.0.1",
    "publisher": "Jser",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showMenu"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showMenu",
                "title": "Show Main Menu"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "fs-extra": "^6.0.1",
        "mongodb-memory-server": "^1.8.0"
    }
}
