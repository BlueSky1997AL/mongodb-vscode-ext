{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": 0,
    "arrow-body-style": 0,
    "no-unused-expressions": 0,
    "no-plusplus": 0,
    "no-console": 0,
    "func-names": 0,
    "comma-dangle": 0,
    "prettier/prettier": ["error", {
      "printWidth": 100,
      "singleQuote": true,
      "trailingComma": "es5"
    }],
    "no-prototype-builtins": 0,
    "prefer-destructuring": 0
  },
  "env": {
    "jasmine": true,
    "jest": true
  },
  "globals": {
    "Class": true,
    "Iterator": true,
    "$Shape": true,
  },
  "plugins": [
    "flowtype",
    "prettier"
  ]
}
